<% if (title) { %>
<h1><%= title %></h1>
<% } else {
    res.redirect('/')
} %>
        
<%- messages() %>    

<% if (errors) { %>
    <ul class="notice">
    <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
<%  }) %>
    </ul>
<% } %>

<h2 class="managementHeader"><%= welcomeMessage %></h2>
<div>You're logged in.</div>

<% if (updateAccountLink) { %>
    <p><a href="<%= updateAccountLink %>">Edit Account Information</a></p>
<% } %>

<%- manageInventorySection %>